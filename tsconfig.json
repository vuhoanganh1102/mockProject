{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/jwt-authentication": [
        "libs/jwt-authentication/src"
      ],
      "@app/jwt-authentication/*": [
        "libs/jwt-authentication/src/*"
      ],
      "@app/core": [
        "libs/core/src"
      ],
      "@app/core/*": [
        "libs/core/src/*"
      ],
      "@app/database-typeorm": [
        "libs/database-typeorm/src"
      ],
      "@app/database-typeorm/*": [
        "libs/database-typeorm/src/*"
      ],
      "@app/send-email": [
        "libs/send-email/src"
      ],
      "@app/send-email/*": [
        "libs/send-email/src/*"
      ],
      "@app/notification": [
        "libs/notification/src"
      ],
      "@app/notification/*": [
        "libs/notification/src/*"
      ],
      "@app/exception-filter": [
        "libs/exception-filter/src"
      ],
      "@app/exception-filter/*": [
        "libs/exception-filter/src/*"
      ],
      "@app/config": [
        "libs/config/src"
      ],
      "@app/config/*": [
        "libs/config/src/*"
      ],
      "@app/helpers": [
        "libs/helpers/src"
      ],
      "@app/helpers/*": [
        "libs/helpers/src/*"
      ]
    }
  }
}